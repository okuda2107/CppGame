{
	"name": "${localWorkspaceFolderBasename}",
	"dockerComposeFile": "docker-compose.yml",
	"service": "dev",
	"containerUser": "vscode",
	"remoteUser": "vscode",
	"workspaceFolder": "/workspace",
	"updateRemoteUserUID": true,
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-vscode.cpptools",
				"ms-vscode.makefile-tools",
				"ms-vscode.cmake-tools"
			]
		}
	}
}
